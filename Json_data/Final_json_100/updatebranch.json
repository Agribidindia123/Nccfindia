  "test_cases": [
    {
      "test_id": "TC-001",
      "category": "Functional - Positive",
      "type": "Positive-CRUD-Integration",
      "description": "Update branch with valid branch_id and valid data",
      "endpoint": "/branch/update_branch/21223a1d-54f6-4155-8305-ea601296baf6",
      "auth_token": "valid",
      "body": {
        "branch_name": "Mumbai East",
        "branch_code": "MBE123",
        "branch_email": "east@mumbai.com",
        "branch_phone": "9876543210",
        "branch_address": "Andheri East, Mumbai"
      },
      "expected_status": 200,
      "expected_response": {
        "status": 200,
        "message": "Branch updated successfully",
        "data": [
          {
            "branch_id": "21223a1d-54f6-4155-8305-ea601296baf6"
          }
        ],
        "total_count": 1
      }
    },

    {
      "test_id": "TC-002",
      "category": "Functional - Negative",
      "type": "Negative-Boundary-DataValidation",
      "description": "Invalid branch_id format",
      "endpoint": "/branch/update_branch/21223a1d-54f6-4155-",
      "body": {},
      "expected_status": 422,
      "expected_response": {
        "status": "error",
        "message": "Validation failed",
        "data": [],
        "total_count": 0
      }
    },

    {
      "test_id": "TC-003",
      "category": "Functional - Negative",
      "type": "Negative-Boundary-CRUD",
      "description": "Branch_id does not exist",
      "endpoint": "/branch/update_branch/bf7cbefe-dca3-470a-a509-f2343eb4f777",
      "body": {},
      "expected_status": 404,
      "expected_response": {
        "detail": "Branch with ID bf7cbefe-dca3-470a-a509-f2343eb4f777 not found"
      }
    },

    {
      "test_id": "TC-004",
      "category": "Authentication - Positive",
      "type": "Positive-Authentication",
      "description": "Access with valid token",
      "endpoint": "/branch/update_branch/21223a1d-54f6-4155-8305-ea601296baf6",
      "auth_token": "valid",
      "body": {
        "branch_name": "Mumbai west"
      },
      "expected_status": 200,
      "expected_response": {
        "status": 200,
        "message": "Branch updated successfully",
        "data": [
          {
            "branch_id": "21223a1d-54f6-4155-8305-ea601296baf6"
          }
        ],
        "total_count": 1
      }
    },

    {
      "test_id": "TC-005",
      "category": "Authentication - Negative",
      "type": "Negative-Authentication-Security",
      "description": "Access without token",
      "endpoint": "/branch/update_branch/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "empty",
      "body": {},
      "expected_status": 401,
      "expected_response": {
        "detail": "Missing or invalid Authorization header"
      }
    },

    {
      "test_id": "TC-006",
      "category": "Authentication - Negative",
      "type": "Negative-Authentication-Security",
      "description": "Invalid token",
      "endpoint": "/branch/update_branch/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "invalid",
      "body": {},
      "expected_status": 401,
      "expected_response": {
        "detail": "Failed to get signing key: Error decoding token headers."
      }
    },

    {
      "test_id": "TC-007",
      "category": "Authentication - Negative",
      "type": "Negative-Authentication-Security",
      "description": "Expired token",
      "endpoint": "/branch/update_branch/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "expired",
      "body": {},
      "expected_status": 401,
      "expected_response": {
        "detail": "Token has expired"
      }
    },

    {
      "test_id": "TC-008",
      "category": "Authentication - Negative",
      "type": "Negative-Authentication-Security",
      "description": "Malformed token",
      "endpoint": "/branch/update_branch/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "malformed",
      "body": {},
      "expected_status": 401,
      "expected_response": {
        "detail": "Failed to get signing key: Error decoding token headers."
      }
    },

    {
      "test_id": "TC-009",
      "category": "Role-Based - Positive",
      "type": "Role-Based-Authorization",
      "description": "Admin update branch",
      "endpoint": "/branch/update_branch/21223a1d-54f6-4155-8305-ea601296baf6",
      "auth_token": "valid_admin",
      "body": {
        "branch_email_id": "west@mumbai.com"
      },
      "expected_status": 200,
      "expected_response": {
        "status": 200,
        "message": "Branch updated successfully",
        "data": [
          {
            "branch_id": "21223a1d-54f6-4155-8305-ea601296baf6"
          }
        ],
        "total_count": 1
      }
    },

    {
      "test_id": "TC-010",
      "category": "Role-Based - Negative",
      "type": "Role-Based-Authorization",
      "description": "Branch user cannot update",
      "endpoint": "/branch/update_branch/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "valid_branch",
      "body": {},
      "expected_status": 403,
      "expected_response": {
        "detail": "Access denied. Required roles: ADMIN, SUPER_ADMIN. Your role: BRANCH"
      }
    },

    {
      "test_id": "TC-011",
      "category": "Role-Based - Negative",
      "type": "Role-Based-Authorization",
      "description": "Aggregator cannot update",
      "endpoint": "/branch/update_branch/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "valid_aggregator",
      "body": {},
      "expected_status": 403,
      "expected_response": {
        "detail": "Access denied. Required roles: ADMIN, SUPER_ADMIN. Your role: BRANCH"
      }
    }
