  "test_cases": [
    {
      "test_id": "TC-001",
      "description": "Positive - Update warehouse to active with valid ID",
      "category": "Positive / Functional",
      "endpoint": "/warehouse/update_warehouse_status/ee86e55c-48e4-4056-9c1a-c061b6e6a9d1?status=1",
