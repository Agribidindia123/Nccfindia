    {
      "test_id": "TC-003",
      "description": "Negative - Non-existing warehouse ID",
      "category": "Negative / Data Validation",
      "endpoint": "/warehouse/update_warehouse_status/ee86e55c-48e4-4056-9c1a-c061b6e6a9d3?status=1",
