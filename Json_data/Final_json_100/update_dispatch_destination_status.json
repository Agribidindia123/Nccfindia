  "test_cases": [
    {
      "test_id": "TC-001",
      "description": "Positive - Toggle dispatch destination status with valid token",
      "category": "Positive | Functional | CRUD",
      "endpoint": "/dispatch_destination/status",
      "auth_token": "valid",
      "body": {
        "dispatch_destination_id": "86a8c46c-87fd-4ddc-885d-e697ca562212",
        "deleted_by": "xyz"
      },
      "expected_status": 200,
      "expected_response_contains": [
        "Dispatch destination status toggled",
        "dispatch_destination_id",
        "status"
      ]
    },
    {
      "test_id": "TC-002",
      "description": "Negative - Expired token",
      "category": "Negative | Authentication",
      "endpoint": "/dispatch_destination/status",
      "auth_token": "expired",
      "body": {
        "dispatch_destination_id": "86a8c46c-87fd-4ddc-885d-e697ca562212",
        "deleted_by": "xyz"
      },
      "expected_status": 401,
      "expected_error_message_substring": "token expired"
    },
    {
      "test_id": "TC-003",
      "description": "Negative - Invalid token",
      "category": "Negative | Authentication",
      "endpoint": "/dispatch_destination/status",
      "auth_token": "invalid",
      "body": {
        "dispatch_destination_id": "86a8c46c-87fd-4ddc-885d-e697ca562212",
        "deleted_by": "xyz"
      },
      "expected_status": 401,
      "expected_error_message_substring": "invalid token"
    },
    {
      "test_id": "TC-004",
      "description": "Negative - Malformed token",
      "category": "Negative | Authentication",
      "endpoint": "/dispatch_destination/status",
      "auth_token": "malformed",
      "body": {
        "dispatch_destination_id": "86a8c46c-87fd-4ddc-885d-e697ca562212",
        "deleted_by": "xyz"
      },
      "expected_status": 401,
      "expected_error_message_substring": "malformed"
    },
    {
      "test_id": "TC-005",
      "description": "Negative - Empty token",
      "category": "Negative | Authorization",
      "endpoint": "/dispatch_destination/status",
      "auth_token": "empty",
      "body": {
        "dispatch_destination_id": "86a8c46c-87fd-4ddc-885d-e697ca562212",
        "deleted_by": "xyz"
      },
      "expected_status": 401,
      "expected_error_message_substring": "token missing"
    },
    {
      "test_id": "TC-006",
      "description": "Negative - Invalid dispatch_destination_id",
      "category": "Negative | Validation",
      "endpoint": "/dispatch_destination/status",
      "auth_token": "valid",
      "body": {
        "dispatch_destination_id": "invalid-id-123",
        "deleted_by": "xyz"
      },
      "expected_status": 400,
      "expected_error_message_substring": "invalid dispatch_destination_id"
    },
    {
      "test_id": "TC-007",
      "description": "Negative - Missing dispatch_destination_id",
      "category": "Negative | Validation",
      "endpoint": "/dispatch_destination/status",
      "auth_token": "valid",
      "body": {
        "deleted_by": "xyz"
      },
      "expected_status": 400,
      "expected_error_message_substring": "dispatch_destination_id required"
    },
    {
      "test_id": "TC-008",
      "description": "Positive Role-Based - Admin toggles status",
      "category": "Positive | Role-Based | Authorization",
      "endpoint": "/dispatch_destination/status",
      "auth_token": "valid_admin",
      "body": {
        "dispatch_destination_id": "86a8c46c-87fd-4ddc-885d-e697ca562212",
        "deleted_by": "admin_user"
      },
      "expected_status": 200,
      "expected_response_contains": [
        "Dispatch destination status toggled"
      ]
    },
    {
      "test_id": "TC-009",
      "description": "Negative Role-Based Access - Aggregator forbidden",
      "category": "Negative | Role-Based | Authorization",
      "endpoint": "/dispatch_destination/status",
      "auth_token": "valid_aggregator",
      "body": {
        "dispatch_destination_id": "86a8c46c-87fd-4ddc-885d-e697ca562212",
        "deleted_by": "aggregator_user"
      },
      "expected_status": 403,
      "expected_error_message_substring": "Forbidden"
    },
    {
      "test_id": "TC-010",
      "description": "Positive Role-Based Access - Branch toggles status",
      "category": "Positive | Role-Based | Authorization",
      "endpoint": "/dispatch_destination/status",
      "auth_token": "valid_branch",
      "body": {
        "dispatch_destination_id": "86a8c46c-87fd-4ddc-885d-e697ca562212",
        "deleted_by": "branch_user"
      },
      "expected_status": 200,
      "expected_response_contains": [
        "Dispatch destination status toggled"
      ]
    },
    {
      "test_id": "TC-011",
      "description": "Negative - Admin expired token",
      "category": "Negative | Authentication",
      "endpoint": "/dispatch_destination/status",
      "auth_token": "expired",
      "body": {
        "dispatch_destination_id": "86a8c46c-87fd-4ddc-885d-e697ca562212",
        "deleted_by": "admin_user"
      },
