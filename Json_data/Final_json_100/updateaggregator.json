      },
      "auth_token": "valid",
      "expected_status": 200,
      "expected_response": "Success - Aggregator details updated"
    },
    {
      "test_id": "TC-002",
      "category": "Functional - Negative",
      "type": "Negative-Boundary-CRUD",
