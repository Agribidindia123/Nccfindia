      "category": "Positive",
      "description": "Fetch farmer count with valid token and no filters",
      "auth_token": "valid",
      "method": "GET",
      "endpoint": "/farmer/count",
      "query_params": {},
      "expected_status": 200,
      "expected_response": {
        "keys_present": ["total_records", "total_pages"]
      }
    },
    {
      "test_id": "TC-002",
      "category": "Positive",
      "description": "Fetch farmer count filtered by active status (2)",
      "auth_token": "valid",
      "method": "GET",
