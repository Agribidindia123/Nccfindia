  "test_cases": [
    {
      "test_id": "TC-001",
      "description": "Positive - Delete collection center with valid ID",
      "category": "Positive / Data Validation",
      "endpoint": "/collection_center/bb8917af-257e-4e76-8bb6-be2f38c3f2e1",
      "auth_token": "valid",
      "expected_status": 200,
      "expected_response_contains": [
        "deleted successfully"
      ]
    },
    {
      "test_id": "TC-002",
      "description": "Negative - Missing Authorization header",
      "category": "Negative / Authentication",
      "endpoint": "/collection_center/bb8917af-257e-4e76-8bb6-be2f38c3f2e1",
      "auth_token": "empty",
      "expected_status": 401,
      "expected_error_message_substring": "Missing or invalid Authorization"
    },
    {
      "test_id": "TC-003",
      "description": "Negative - Expired Authorization token",
      "category": "Negative / Authentication",
      "endpoint": "/collection_center/bb8917af-257e-4e76-8bb6-be2f38c3f2e1",
      "auth_token": "expired",
      "expected_status": 401,
      "expected_error_message_substring": "Token expired"
    },
    {
      "test_id": "TC-004",
      "description": "Negative - Invalid Authorization token",
      "category": "Negative / Authentication",
      "endpoint": "/collection_center/bb8917af-257e-4e76-8bb6-be2f38c3f2e1",
      "auth_token": "invalid",
      "expected_status": 401,
      "expected_error_message_substring": "Invalid token"
    },
    {
      "test_id": "TC-005",
      "description": "Negative - Malformed Authorization token",
      "category": "Negative / Authentication",
      "endpoint": "/collection_center/bb8917af-257e-4e76-8bb6-be2f38c3f2e1",
      "auth_token": "malformed",
      "expected_status": 401,
      "expected_error_message_substring": "Invalid token format"
    },
    {
      "test_id": "TC-006",
      "description": "Negative - Malformed collection_center_id",
      "category": "Negative / Input Validation",
      "endpoint": "/collection_center/bb8917af-257e-4e7",
      "auth_token": "valid",
      "expected_status": 422,
      "expected_error_message_substring": "Validation failed"
    },
    {
      "test_id": "TC-007",
      "description": "Boundary - Extremely long collection_center_id (100 chars)",
      "category": "Boundary / Input Validation",
      "endpoint": "/collection_center/AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA",
      "auth_token": "valid",
      "expected_status": 422,
      "expected_error_message_substring": "Validation failed"
    },
    {
      "test_id": "TC-008",
      "description": "Negative - Non-existing collection_center_id",
      "category": "Negative / Data Validation",
      "endpoint": "/collection_center/00000000-0000-0000-0000-000000000000",
      "auth_token": "valid",
      "expected_status": 404,
      "expected_error_message_substring": "No records found"
    },
    {
      "test_id": "TC-009",
      "description": "Positive Role-Based - Admin not allowed to delete collection center",
      "category": "Positive / Role-Based Access",
      "endpoint": "/collection_center/bb8917af-257e-4e76-8bb6-be2f38c3f2e1",
      "auth_token": "valid_admin",
      "expected_status": 403,
      "expected_error_message_substring": "Forbidden"
    },
    {
      "test_id": "TC-010",
      "description": "Negative Role-Based - Aggregator deletes collection center successfully",
      "category": "Negative / Role-Based Access",
      "endpoint": "/collection_center/bb8917af-257e-4e76-8bb6-be2f38c3f2e1",
      "auth_token": "valid_aggregator",
      "expected_status": 200,
      "expected_response_contains": [
        "deleted successfully"
      ]
    },
    {
      "test_id": "TC-011",
      "description": "Negative Role-Based - Branch user not allowed to delete collection center",
      "category": "Negative / Role-Based Access",
      "endpoint": "/collection_center/bb8917af-257e-4e76-8bb6-be2f38c3f2e1",
      "auth_token": "valid_branch",
      "expected_status": 403,
      "expected_error_message_substring": "Forbidden"
    }
  ]
}
