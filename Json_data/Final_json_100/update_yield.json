  "test_cases": [
    {
      "test_id": "TC-001",
      "description": "Positive - Update yield with valid token",
      "category": "Positive | Functional | CRUD",
      "endpoint": "/yield/update_yield/9aa11ccb-1bcc-4d7c-ba62-12e2e892807f",
      "auth_token": "valid",
      "body": {
        "quantity_per_hectare_in_qtl": 1,
        "update_date": "2025-10-09T07:55:56.718Z"
      },
      "expected_status": 200,
      "expected_response_contains": [
        "Yield updated successfully",
        "yield_id"
      ]
    },
    {
      "test_id": "TC-002",
      "description": "Negative - No token provided",
      "category": "Negative | Authorization",
      "endpoint": "/yield/update_yield/9aa11ccb-1bcc-4d7c-ba62-12e2e892807f",
      "auth_token": "empty",
      "body": {
        "quantity_per_hectare_in_qtl": 1,
        "update_date": "2025-10-09T07:55:56.718Z"
      },
      "expected_status": 401,
      "expected_error_message_substring": "Missing or invalid Authorization"
    },
    {
      "test_id": "TC-003",
      "description": "Negative - Expired token",
      "category": "Negative | Authentication",
      "endpoint": "/yield/update_yield/9aa11ccb-1bcc-4d7c-ba62-12e2e892807f",
      "auth_token": "expired",
      "body": {
        "quantity_per_hectare_in_qtl": 1,
        "update_date": "2025-10-09T07:55:56.718Z"
      },
      "expected_status": 401,
      "expected_error_message_substring": "Token expired"
    },
    {
      "test_id": "TC-004",
      "description": "Negative - Malformed token",
      "category": "Negative | Authentication",
      "endpoint": "/yield/update_yield/9aa11ccb-1bcc-4d7c-ba62-12e2e892807f",
      "auth_token": "malformed",
      "body": {
        "quantity_per_hectare_in_qtl": 1,
        "update_date": "2025-10-09T07:55:56.718Z"
      },
      "expected_status": 401,
      "expected_error_message_substring": "Invalid token"
    },
    {
      "test_id": "TC-005",
      "description": "Negative - Missing required field quantity_per_hectare_in_qtl",
      "category": "Negative | Validation",
      "endpoint": "/yield/update_yield/9aa11ccb-1bcc-4d7c-ba62-12e2e892807f",
      "auth_token": "valid",
      "body": {
        "update_date": "2025-10-09T13:18:46.196Z"
      },
      "expected_status": 422,
      "expected_error_message_substring": "quantity_per_hectare_in_qtl is required"
    },
    {
      "test_id": "TC-006",
      "description": "Positive Role-Based Access - Admin updates yield successfully",
      "category": "Positive | Role-Based | Authorization",
      "endpoint": "/yield/update_yield/9aa11ccb-1bcc-4d7c-ba62-12e2e892807f",
      "auth_token": "valid_admin",
      "body": {
        "quantity_per_hectare_in_qtl": 2,
        "update_date": "2025-10-09T08:55:56.718Z"
      },
      "expected_status": 200,
      "expected_response_contains": [
        "Yield updated successfully",
        "yield_id"
      ]
    },
    {
      "test_id": "TC-007",
      "description": "Negative Role-Based Access - Aggregator user forbidden to update yield",
      "category": "Positive | Role-Based | Authorization",
      "endpoint": "/yield/update_yield/9aa11ccb-1bcc-4d7c-ba62-12e2e892807f",
      "auth_token": "valid_aggregator",
      "body": {
        "quantity_per_hectare_in_qtl": 3,
        "update_date": "2025-10-09T09:55:56.718Z"
      },
      "expected_status": 403,
       "expected_error_message_substring": "Forbidden"
    },
    {
      "test_id": "TC-008",
      "description": "Negative Role-Based Access - Branch user forbidden to update yield",
      "category": "Negative | Role-Based | Authorization",
      "endpoint": "/yield/update_yield/9aa11ccb-1bcc-4d7c-ba62-12e2e892807f",
      "auth_token": "valid_branch",
      "body": {
        "quantity_per_hectare_in_qtl": 4,
        "update_date": "2025-10-09T10:55:56.718Z"
      },
      "expected_status": 403,
      "expected_error_message_substring": "Forbidden"
