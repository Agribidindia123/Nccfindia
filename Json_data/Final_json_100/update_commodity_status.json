  "test_cases": [
    {
      "test_id": "TC-001",
      "description": "Positive - Update commodity status with valid ID and valid body",
      "endpoint": "/commodity_master/update_commodity_status/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "valid",
      "path_params": {
        "commodity_master_id": "bf7cbefe-dca3-470a-a509-f2343eb4f128"
      },
      "request_body": {
        "commodity_status": 0
      },
      "expected_status": 200,
      "expected_response": {
        "status": 200,
        "message": "Commodity status updated successfully",
        "data": null,
        "total_count": null
      }
    },
    {
      "test_id": "TC-002",
      "description": "Negative (Invalid ID) - Update with non-existing commodity ID",
      "endpoint": "/commodity_master/update_commodity_status/bf0cbefe-dca0-000a-a000-f0000eb4f000",
      "auth_token": "valid",
      "path_params": {
        "commodity_master_id": "bf0cbefe-dca0-000a-a000-f0000eb4f000"
      },
      "request_body": {
        "commodity_status": 1
      },
      "expected_status": 404,
      "expected_response": {
        "detail": "Commodity not found"
      }
    },
    {
      "test_id": "TC-003",
      "description": "Negative (Invalid Format) - Commodity ID is not UUID format",
      "endpoint": "/commodity_master/update_commodity_status/c18bcebfa2e54e4c83f4337d4b4cf3c2",
      "auth_token": "valid",
      "path_params": {
        "commodity_master_id": "c18bcebfa2e54e4c83f4337d4b4cf3c2"
      },
      "request_body": {
        "commodity_status": 1
      },
      "expected_status": 404,
      "expected_response": {
        "detail": "Invalid UUID format for commodity_master_id"
      }
    },
    {
      "test_id": "TC-004",
      "description": "Negative (Missing Field) - Request body without commodity_status",
      "endpoint": "/commodity_master/update_commodity_status/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "valid",
      "path_params": {
        "commodity_master_id": "bf7cbefe-dca3-470a-a509-f2343eb4f128"
      },
      "request_body": {},
      "expected_status": 422,
      "expected_response": {
        "detail": "Validation failed. Missing required field: commodity_status"
      }
    },
    {
      "test_id": "TC-005",
      "description": "Negative (Invalid Status Value) - Send invalid status value",
      "endpoint": "/commodity_master/update_commodity_status/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "valid",
      "path_params": {
        "commodity_master_id": "bf7cbefe-dca3-470a-a509-f2343eb4f128"
      },
      "request_body": {
        "commodity_status": "active"
      },
      "expected_status": 422,
      "expected_response": {
        "detail": "Invalid data type for commodity_status. Expected integer."
      }
    },
    {
      "test_id": "TC-006",
      "description": "Negative (Auth) - Missing authentication token",
      "endpoint": "/commodity_master/update_commodity_status/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": null,
      "request_body": {
        "commodity_status": 1
      },
      "expected_status": 401,
      "expected_response": {
        "detail": "Missing or invalid Authorization header"
      }
    },
    {
      "test_id": "TC-007",
      "description": "Negative (Auth) - Expired/invalid authentication token",
      "endpoint": "/commodity_master/update_commodity_status/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "expired",
      "request_body": {
        "commodity_status": 1
      },
      "expected_status": 401,
      "expected_response": {
        "detail": "Token expired"
      }
    },
    {
      "test_id": "TC-008",
      "description": "Negative (Auth) - Invalid authentication token format",
      "endpoint": "/commodity_master/update_commodity_status/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "invalid",
      "request_body": {
        "commodity_status": 1
      },
      "expected_status": 401,
      "expected_response": {
        "detail": "Invalid token"
      }
    },
    {
      "test_id": "TC-009",
      "description": "Role-Based - Admin: Can update commodity status successfully",
      "endpoint": "/commodity_master/update_commodity_status/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "valid_admin",
      "request_body": {
        "commodity_status": 1
      },
      "expected_status": 200,
      "expected_response": {
        "message": "Commodity status updated successfully"
      }
    },
    {
      "test_id": "TC-010",
      "description": "Role-Based - Branch: Cannot update commodity status",
      "endpoint": "/commodity_master/update_commodity_status/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "valid_branch",
      "request_body": {
        "commodity_status": 1
      },
      "expected_status": 403,
      "expected_response": {
        "detail": "Access denied"
      }
    },
    {
      "test_id": "TC-011",
      "description": "Role-Based - Aggregator: Cannot update commodity status",
      "endpoint": "/commodity_master/update_commodity_status/bf7cbefe-dca3-470a-a509-f2343eb4f128",
      "auth_token": "valid_aggregator",
      "request_body": {
        "commodity_status": 1
      },
      "expected_status": 403,
      "expected_response": {
        "detail": "Access denied"
      }
    }
