  "test_cases": [
    {
      "test_id": "TC-001",
      "description": "Positive - Update transport service with valid data",
      "category": "Positive / Data Validation",
      "endpoint": "/transport_service/update_transport_service/6b830fb6-877e-4496-8ffc-1db41c20e5a5",
      "auth_token": "valid_aggregator",
      "body": {
        "transport_service_name": "Tarun Logistics",
        "gst_number": "27ABCDE1234A1Z5",
        "pan_number": "AAAPX1234A",
        "transport_owner_name": "Tarun Shikawat",
        "mobile_number": "9876543234",
        "email_id": "user12@example.com",
        "address": "Jaipur",
        "pin_code": "911420",
        "city_master_id": "ddb9b1d2-52af-4a93-ac58-f7fdfba1bd24",
        "district_master_id": "479d6dff-668a-4a4a-99a9-6a5f5d838c29",
        "state_master_id": "f36ed246-2721-4c2a-a25d-27f70f5d8be6"
      },
      "expected_status": 200,
      "expected_response_contains": [
        "Transport service updated successfully"
      ]
    },
    {
      "test_id": "TC-002",
      "description": "Negative - Invalid GST number format",
      "category": "Negative / Input Validation",
      "endpoint": "/transport_service/update_transport_service/6b830fb6-877e-4496-8ffc-1db41c20e5a5",
      "auth_token": "valid",
      "body": {
        "gst_number": "INVALIDGST123"
      },
      "expected_status": 422,
      "expected_error_message_substring": "Invalid GST format"
    },
    {
      "test_id": "TC-003",
      "description": "Negative - Invalid PAN number format",
      "category": "Negative / Input Validation",
      "endpoint": "/transport_service/update_transport_service/6b830fb6-877e-4496-8ffc-1db41c20e5a5",
      "auth_token": "valid",
      "body": {
        "pan_number": "AD45R"
      },
      "expected_status": 422,
      "expected_error_message_substring": "Invalid PAN format"
    },
    {
      "test_id": "TC-004",
      "description": "Negative - Invalid mobile number",
      "category": "Negative / Input Validation",
      "endpoint": "/transport_service/update_transport_service/6b830fb6-877e-4496-8ffc-1db41c20e5a5",
      "auth_token": "valid",
      "body": {
        "mobile_number": "12345"
      },
      "expected_status": 422,
      "expected_error_message_substring": "Invalid mobile number"
    },
    {
      "test_id": "TC-005",
      "description": "Negative - Invalid email format",
      "category": "Negative / Input Validation",
      "endpoint": "/transport_service/update_transport_service/6b830fb6-877e-4496-8ffc-1db41c20e5a5",
      "auth_token": "valid",
      "body": {
        "email_id": "invalid-email-format"
      },
      "expected_status": 422,
      "expected_error_message_substring": "Invalid email format"
    },
    {
      "test_id": "TC-006",
      "description": "Negative - Missing Authorization header",
      "category": "Negative / Authentication",
      "endpoint": "/transport_service/update_transport_service/6b830fb6-877e-4496-8ffc-1db41c20e5a5",
      "auth_token": "empty",
      "body": {},
      "expected_status": 401,
      "expected_error_message_substring": "Missing or invalid Authorization"
    },
    {
      "test_id": "TC-007",
      "description": "Negative - Expired Authorization token",
      "category": "Negative / Authentication",
      "endpoint": "/transport_service/update_transport_service/6b830fb6-877e-4496-8ffc-1db41c20e5a5",
      "auth_token": "expired",
      "body": {},
      "expected_status": 401,
      "expected_error_message_substring": "Token expired"
    },
    {
      "test_id": "TC-008",
      "description": "Negative - Malformed transport_service_id",
      "category": "Negative / Input Validation",
      "endpoint": "/transport_service/update_transport_service/6b830fb6-877e-4",
      "auth_token": "valid",
      "body": {},
      "expected_status": 422,
      "expected_error_message_substring": "Validation failed"
    },
    {
      "test_id": "TC-009",
      "description": "Boundary - Extremely long transport_service_id (100 chars)",
      "category": "Boundary / Input Validation",
      "endpoint": "/transport_service/update_transport_service/AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA",
      "auth_token": "valid",
      "body": {},
      "expected_status": 422,
      "expected_error_message_substring": "Validation failed"
    },
    {
      "test_id": "TC-010",
      "description": "Negative - Non-existing transport_service_id",
      "category": "Negative / Data Validation",
      "endpoint": "/transport_service/update_transport_service/00000000-0000-0000-0000-000000000000",
      "auth_token": "valid",
      "body": {},
      "expected_status": 422,
      "expected_error_message_substring": "No records found"
    },
    {
      "test_id": "TC-011",
      "description": "Role-Based - Admin user cannot update transport service",
      "category": "Negative / Role-Based Access Control",
      "endpoint": "/transport_service/update_transport_service/6b830fb6-877e-4496-8ffc-1db41c20e5a5",
      "auth_token": "valid_admin",
      "body": {
        "transport_service_name": "Admin Try Update",
        "gst_number": "27ABCDE1234A1Z5",
        "mobile_number": "9876543210"
      },
      "expected_status": 403,
      "expected_error_message_substring": "Permission denied"
    },
    {
      "test_id": "TC-012",
      "description": "Role-Based - Branch user cannot update transport service",
      "category": "Negative / Role-Based Access Control",
      "endpoint": "/transport_service/update_transport_service/6b830fb6-877e-4496-8ffc-1db41c20e5a5",
      "auth_token": "valid_branch",
      "body": {
        "transport_service_name": "Branch Try Update",
        "gst_number": "27ABCDE1234A1Z5",
        "mobile_number": "9876543211"
      },
      "expected_status": 403,
      "expected_error_message_substring": "Permission denied"
    },
    {
      "test_id": "TC-013",
      "description": "Role-Based - Aggregator user can successfully update transport service",
      "category": "Positive / Role-Based Access Control",
      "endpoint": "/transport_service/update_transport_service/6b830fb6-877e-4496-8ffc-1db41c20e5a5",
      "auth_token": "valid_aggregator",
      "body": {
        "transport_service_name": "Aggregator Updated Service",
        "gst_number": "27ABCDE1234A1Z5",
