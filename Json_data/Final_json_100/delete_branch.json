  "test_cases": [
    {
      "test_id": "TC-001",
      "description": "Positive - Delete branch with valid branch_id and valid token",
      "category": "Positive / CRUD Operations",
      "endpoint": "/branch/delete_branch/19326db4-cfbc-461e-a57e-2a2498e7940d",
      "auth_token": "valid",
      "request_data": {},
      "expected_status": 200,
      "expected_response_contains": [
        "Branch deleted successfully",
        "data",
        "total_count"
      ]
    },
    {
      "test_id": "TC-002",
      "description": "Negative - Delete branch with non-existing branch_id",
      "category": "Negative / Data Validation",
      "endpoint": "/branch/delete_branch/00000000-0000-0000-0000-000000000000",
      "auth_token": "valid",
      "request_data": {},
      "expected_status": 404,
      "expected_error_message_substring": "Branch not found"
    },
    {
      "test_id": "TC-003",
      "description": "Negative - Delete branch with malformed branch_id",
      "category": "Negative / Input Validation",
      "endpoint": "/branch/delete_branch/19326db4-cfbc-",
      "auth_token": "valid",
      "request_data": {},
      "expected_status": 422,
      "expected_error_message_substring": "Validation failed"
    },
    {
      "test_id": "TC-004",
      "description": "Negative - Missing Authorization header",
      "category": "Negative / Authentication",
      "endpoint": "/branch/delete_branch/19326db4-cfbc-461e-a57e-2a2498e7940d",
      "auth_token": "empty",
      "request_data": {},
      "expected_status": 401,
      "expected_error_message_substring": "Missing or invalid Authorization"
    },
    {
      "test_id": "TC-005",
      "description": "Negative - Invalid Authorization token",
      "category": "Negative / Authentication",
      "endpoint": "/branch/delete_branch/19326db4-cfbc-461e-a57e-2a2498e7940d",
      "auth_token": "invalid",
      "request_data": {},
      "expected_status": 401,
      "expected_error_message_substring": "Invalid token"
    },
    {
      "test_id": "TC-006",
      "description": "Negative - Expired Authorization token",
      "category": "Negative / Authentication",
      "endpoint": "/branch/delete_branch/19326db4-cfbc-461e-a57e-2a2498e7940d",
      "auth_token": "expired",
      "request_data": {},
      "expected_status": 401,
      "expected_error_message_substring": "Token expired"
    },
    {
      "test_id": "TC-007",
      "description": "Negative - Malformed Authorization token",
      "category": "Negative / Authentication",
      "endpoint": "/branch/delete_branch/19326db4-cfbc-461e-a57e-2a2498e7940d",
      "auth_token": "malformed",
      "request_data": {},
      "expected_status": 401,
      "expected_error_message_substring": "Malformed token"
    },
    {
      "test_id": "TC-008",
      "description": "Boundary - Extremely long branch_id (100 chars)",
      "category": "Boundary / Input Validation",
      "endpoint": "/branch/delete_branch/aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa",
      "auth_token": "valid",
      "request_data": {},
      "expected_status": 422,
      "expected_error_message_substring": "Validation failed"
    },
    {
      "test_id": "TC-009",
      "description": "Role-Based - Admin: Can delete branch successfully",
      "category": "Role-Based / Authorization",
      "endpoint": "/branch/delete_branch/19326db4-cfbc-461e-a57e-2a2498e7940d",
      "auth_token": "valid_admin",
      "request_data": {},
      "expected_status": 200,
      "expected_response_contains": [
        "Branch deleted successfully",
        "data"
      ]
    },
    {
      "test_id": "TC-010",
      "description": "Role-Based - Branch: Cannot delete branch",
      "category": "Role-Based / Authorization",
      "endpoint": "/branch/delete_branch/19326db4-cfbc-461e-a57e-2a2498e7940d",
      "auth_token": "valid_branch",
      "request_data": {},
      "expected_status": 403,
      "expected_error_message_substring": "Access denied"
    },
    {
      "test_id": "TC-011",
      "description": "Role-Based - Aggregator: Cannot delete branch",
      "category": "Role-Based / Authorization",
      "endpoint": "/branch/delete_branch/19326db4-cfbc-461e-a57e-2a2498e7940d",
      "auth_token": "valid_aggregator",
      "request_data": {},
      "expected_status": 403,
      "expected_error_message_substring": "Access denied"
    }
  ]
}
