{
  "base_url": "https://uat-api.nccfindia.com",
  "method": "GET",
  "headers": {
    "Content-Type": "application/json"
  },
  "tokens": {
    "valid": "<fetched_from_login_api>",
    "invalid": "iXd32fC8Di",
    "empty": "",
    "expired": "iXd32fC8Dx",
    "malformed": "iXd32fC8D!",
    "valid_admin": "<fetched_admin_token>",
    "valid_aggregator": "<fetched_aggregator_token>",
    "valid_branch": "<fetched_branch_token>",
    "valid_id": "<fetched_from_login_api>",
    "invalid_id": "<replace_with_id_token_here>",
    "expired_id": "<replace_with_id_token_here>",
    "malformed_id": "<replace_with_id_token_here>",
    "valid_id_id": "<replace_with_id_token_here>",
    "invalid_id_id": "<replace_with_id_token_here>",
    "expired_id_id": "<replace_with_id_token_here>",
    "malformed_id_id": "<replace_with_id_token_here>"
  },
  "test_cases": [
    {
      "test_id": "TC-001",
      "description": "Positive - CRUD Read: Fetch commodity by valid ID (Integration & Functional Test)",
      "endpoint": "/commodity_master/get_commodity/{commodity_master_id}",
      "auth_token": "valid",
      "path_params": {
        "commodity_master_id": "bf7cbefe-dca3-470a-a509-f2343eb4f128"
      },
      "expected_status": 200,
      "expected_response": {
        "message": "Commodity retrieved successfully"
      }
    },
    {
      "test_id": "TC-002",
      "description": "Negative - Data Validation: Commodity ID does not exist in DB (Functional & Boundary Test)",
      "endpoint": "/commodity_master/get_commodity/{commodity_master_id}",
      "auth_token": "valid",
      "path_params": {
        "commodity_master_id": "bf7cbefe-dca3-470a-a509-f2343eb4f100"
      },
      "expected_status": 404,
      "expected_response": {
        "detail": "Commodity not found"
      }
    },
    {
      "test_id": "TC-003",
      "description": "Negative - Data Validation: Commodity ID is not UUID format (Functional & Validation Test)",
      "endpoint": "/commodity_master/get_commodity/{commodity_master_id}",
      "auth_token": "valid",
      "path_params": {
        "commodity_master_id": "123"
      },
      "expected_status": 404
    },
    {
      "test_id": "TC-004",
      "description": "Negative - Authentication & Authorization: Missing authentication token (Security Test)",
      "endpoint": "/commodity_master/get_commodity/{commodity_master_id}",
      "auth_token": "empty",
      "path_params": {
        "commodity_master_id": "bf7cbefe-dca3-470a-a509-f2343eb4f128"
      },
      "expected_status": 401,
      "expected_response": {
        "detail": "Missing or invalid Authorization header"
      }
    },
    {
      "test_id": "TC-005",
      "description": "Negative - Authentication & Authorization: Invalid authentication token (Security Test)",
      "endpoint": "/commodity_master/get_commodity/{commodity_master_id}",
      "auth_token": "invalid",
      "path_params": {
        "commodity_master_id": "bf7cbefe-dca3-470a-a509-f2343eb4f128"
      },
      "expected_status": 401,
      "expected_response": {
        "detail": "Invalid token. Expected: iXd32fC8Dv, Got: iXd32fC8Di"
      }
    },
    {
      "test_id": "TC-006",
      "description": "Negative - Authentication & Authorization: Expired authentication token (Security Test & Boundary Case)",
      "endpoint": "/commodity_master/get_commodity/{commodity_master_id}",
      "auth_token": "expired",
      "path_params": {
        "commodity_master_id": "bf7cbefe-dca3-470a-a509-f2343eb4f128"
      },
      "expected_status": 401,
      "expected_response": {
        "detail": "Token expired"
      }
    },
    {
      "test_id": "TC-007",
      "description": "Negative - Authentication & Authorization: Malformed authentication token (Security Test & Boundary Case)",
      "endpoint": "/commodity_master/get_commodity/{commodity_master_id}",
      "auth_token": "malformed",
      "path_params": {
        "commodity_master_id": "bf7cbefe-dca3-470a-a509-f2343eb4f128"
      },
      "expected_status": 401,
      "expected_response": {
        "detail": "Malformed token"
      }
    },
    {
      "test_id": "TC-008",
      "description": "Positive - Role-Based Access: Admin successfully fetches commodity by valid ID (Authorization Test)",
      "endpoint": "/commodity_master/get_commodity/{commodity_master_id}",
      "auth_token": "valid_admin",
      "path_params": {
        "commodity_master_id": "bf7cbefe-dca3-470a-a509-f2343eb4f128"
      },
      "expected_status": 200,
      "expected_response": {
        "message": "Commodity retrieved successfully"
      }
    },
    {
      "test_id": "TC-009",
      "description": "Positive - Role-Based Access: Aggregator successfully fetches commodity by valid ID (Authorization Test)",
      "endpoint": "/commodity_master/get_commodity/{commodity_master_id}",
      "auth_token": "valid_aggregator",
      "path_params": {
        "commodity_master_id": "bf7cbefe-dca3-470a-a509-f2343eb4f128"
      },
      "expected_status": 200,
      "expected_response": {
        "message": "Commodity retrieved successfully"
      }
    },
    {
      "test_id": "TC-010",
      "description": "Positive - Role-Based Access: Branch successfully fetches commodity by valid ID (Authorization Test)",
      "endpoint": "/commodity_master/get_commodity/{commodity_master_id}",
      "auth_token": "valid_branch",
      "path_params": {
        "commodity_master_id": "bf7cbefe-dca3-470a-a509-f2343eb4f128"
      },
      "expected_status": 200,
      "expected_response": {
        "message": "Commodity retrieved successfully"
      }
    }
  ]
}
