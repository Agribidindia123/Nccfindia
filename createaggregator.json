{
  "base_url": "https://uat-api.nccfindia.com",
  "method": "POST",
  "headers": {
    "Content-Type": "application/json"
  },
  "tokens": {
    "valid": "iXd32fC8Dv",
    "invalid": "iXd32fC8Dm",
    "expired": "your_expired_token_here",
    "malformed": "invalid_token"
  },
  "test_cases": [
    {
      "test_id": "TC-001",
      "description": "Create aggregator with valid required fields — Positive, CRUD Operation, Data Validation",
      "endpoint": "/aggregator/create",
      "request_body": {
        "nccf_branch_id": "03bd05a0-0213-4864-8813-1ffdd25b5063",
        "city_master_id": "dc6925f8-aa8b-4cf2-8d48-624c71aeaba8",
        "district_master_id": "abe73aa6-4d21-4a3c-bcca-51e5a374e1a4",
        "state_master_id": "52d700ac-efbd-447a-85a5-536d0d90056e",
        "email_id": "Karwarpacs@gmail.com",
        "mobile_number": "8567898767"
      },
      "auth_token": "valid",
      "expected_status": 200,
      "expected_response": "Aggregator created successfully"
    },
    {
      "test_id": "TC-002",
      "description": "Missing required field: email_id — Negative, Data Validation",
      "endpoint": "/aggregator/create",
      "request_body": {
        "nccf_branch_id": "03bd05a0-0213-4864-8813-1ffdd25b5063",
        "city_master_id": "dc6925f8-aa8b-4cf2-8d48-624c71aeaba8",
